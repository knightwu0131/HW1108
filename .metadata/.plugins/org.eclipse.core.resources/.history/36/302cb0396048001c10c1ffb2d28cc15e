
public class test_p14 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Car car1;
		car1=new Car();
		 car1.setNum(1234);
		 car1.setGas(20.5);
		 
		 car1.showCar();
		
	}

}
class Car
{
	int num;
	double gas; 
	void setNum(int n) {
		num=n;
	System.out.println("將車號設為"+num);
	}
	void setNum(double g) {
		gas=g;
        System.out.println("將汽油設為"+gas);

	}
    

}